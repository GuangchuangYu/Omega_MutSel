## SJS. shell script to vary numaa from 4->7. starts with NUMAA=4

# establish initial condition variable
start=4

# submit initial NUMAA=4 condition
qsub optima.qsub

# submit lots
for ((i=4;i<7;i++)); do
    next=$[i+1]
    sed -i "s/NUMAA=$i/NUMAA=$next/g" optima.qsub 
    qsub optima.qsub
done

# Reset
sed -i "s/NUMAA=$next/NUMAA=$start/g" optima.qsub


